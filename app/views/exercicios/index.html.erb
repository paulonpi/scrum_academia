<p id="notice"><%= notice %></p>

<h1>Listing Exercicios</h1>

<table>
  <thead>
    <tr>
      <th>Grupo musc</th>
      <th>Dia</th>
      <th>Serie</th>
      <th>Repeticoes</th>
      <th>Kg</th>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exercicios.each do |exercicio| %>
      <tr>
        <td><%= exercicio.grupo_musc %></td>
        <td><%= exercicio.dia %></td>
        <td><%= exercicio.serie %></td>
        <td><%= exercicio.repeticoes %></td>
        <td><%= exercicio.kg %></td>
        <td><%= exercicio.nome %></td>
        <td><%= link_to 'Show', exercicio %></td>
        <td><%= link_to 'Edit', edit_exercicio_path(exercicio) %></td>
        <td><%= link_to 'Destroy', exercicio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exercicio', new_exercicio_path %>
