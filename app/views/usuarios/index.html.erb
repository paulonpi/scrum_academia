<div class="content-page">
   <!-- Start content -->
   <div class="content">
      <div class="container">
         <div class="row">
            <div class="col-lg-12">
               <div class="card-box">
                  <div class="dropdown pull-right">
                     <a href="#" class="dropdown-toggle card-drop" data-toggle="dropdown" aria-expanded="false">
                     <i class="zmdi zmdi-more-vert"></i>
                     </a>
                     <ul class="dropdown-menu" role="menu">
                        <li><a href="#">Action</a></li>
                        <li><a href="#">Another action</a></li>
                        <li><a href="#">Something else here</a></li>
                        <li class="divider"></li>
                        <li><a href="#">Separated link</a></li>
                     </ul>
                  </div>
                  <h4 class="header-title m-t-0 m-b-30">Listagem de Alunos</h4><%= link_to 'New Usuario', new_usuario_path %>
                  <p class="text-muted font-13 m-b-15">
                   Todos os cadastros de alunos.
                  </p>
                  <table class="table m-0">
                     <thead>
                        <tr>
                           <th>ID</th>
                           <th>Nome</th>
                           <th>Endereco</th>
                           <th>Telefone</th>
                           <th>Telefone responsavel</th>
                           <th>Data nasc</th>
                           <th>Cref</th>
                           <th>Tipo usuario</th>
                           
                        </tr>
                     </thead>
                     <tbody>
                        <% @usuarios.each do |usuario| %>
      <tr>
         <td><%= usuario.nome %></td>
         <td><%= usuario.endereco %></td>
         <td><%= usuario.telefone %></td>
         <td><%= usuario.telefone_responsavel %></td>
         <td><%= usuario.data_nasc %></td>
         <td><%= usuario.cref %></td>
         <td><%= usuario.tipo_usuario %></td>
         <td><%= link_to 'Show', usuario %></td>
         <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
         <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      <% end %>
                       
                          
                     </tbody>

                  </table>
               </div>

            </div>
            <!-- end col -->
         </div>
         <!-- End row -->
       
      </div>
      <!-- container -->
   </div>
   <!-- content -->
  
</div>
